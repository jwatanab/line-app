<!doctype html>
<html lang="ja">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="/stylesheets/style.css">
  <script src="/javascripts/line_form.js"></script>
  <title>Line App</title>
</head>

<body>

  <div id="ejsContent">
    <% for(var i in content){ %>
      <% var index = content[i] %>
        <div class="ejsContentName">
          <%= index.name %>
        </div>
        <div class="ejsContentCreate_at">
          <%= index.create_at %>
        </div>
        <div class="ejsContentText">
          <%= index.text %>
        </div>
        <div class="ejsContentObject">
          <%= index.object %>
        </div>
        <% } %>
  </div>

  <div id="root"></div>
</body>

</html>